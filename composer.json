{
    "name": "gingdev/iaskai",
    "version": "1.3.2",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Gingdev\\IAskAI\\": "src/"
        },
        "files": ["src/Internal/functions.php"]
    },
    "authors": [
        {
            "name": "ging-dev",
            "email": "thanh1101dev@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "league/html-to-markdown": "^5.1",
        "amphp/websocket-client": "^2.0",
        "symfony/event-dispatcher": "^7.2",
        "amphp/http-client-cookies": "^2.0",
        "symfony/dom-crawler": "^7.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.12",
        "pestphp/pest": "^3.1"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
